{
    "name": "myprofile-templates",
    "private": false,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "npm run node:generate:json && vite --host 0.0.0.0 --port 3000",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "node:copy-data": "cp data/data.yaml.dist data/data.yaml",
        "node:generate:json": "ts-node ./node/parse.ts",
        "node:generate:pdf": "ts-node ./node/pdf.ts",
        "node:init": "npm run node:copy-data && node:generate:json"
    },
    "devDependencies": {
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3",
        "vite": "^4.1.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.3.0",
        "@types/node": "^18.15.2",
        "bulma": "^0.9.4",
        "puppeteer": "^19.7.5",
        "sass": "^1.59.2",
        "yaml": "^2.2.1"
    }
}
