{% extends 'profile/base.html.twig' %}

{% block body %}
    <h1>Socialnetworking</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ socialNetworking.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ socialNetworking.name }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ socialNetworking.icon }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_socialnetworking_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_socialnetworking_edit', { 'id': socialNetworking.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
