{
    "name": "eerison/myprofile",
    "license": "GPL-3.0-only",
    "type": "project",
    "description": "My profile",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "api-platform/core": "2.6.x-dev",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.11",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "lexik/jwt-authentication-bundle": "^2.14",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.0",
        "sentry/sentry-symfony": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "^5.2",
        "symfony/console": "^v5.2",
        "symfony/dotenv": "^v5.2",
        "symfony/expression-language": "^v5.2",
        "symfony/flex": "^1.3",
        "symfony/framework-bundle": "^v5.2",
        "symfony/http-client": "^v5.2",
        "symfony/mailgun-mailer": "^v5.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "^v5.2",
        "symfony/translation": "^v5.2",
        "symfony/validator": "^v5.2",
        "symfony/yaml": "^v5.2",
        "symfonycasts/reset-password-bundle": "^1.2",
        "symfonycasts/verify-email-bundle": "^1.2"
    },
    "require-dev": {
        "eerison/pest-plugin-api-platform": "^1.0",
        "hautelook/alice-bundle": "^2.9",
        "justinrainbow/json-schema": "^5.2",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3.0",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.12.9",
        "slevomat/coding-standard": "^7.0",
        "spatie/pest-plugin-snapshots": "^1.1",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^5.3",
        "symfony/css-selector": "^5.3",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.3",
        "symfony/web-profiler-bundle": "^5.3"
    },
    "minimum-stability": "beta",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": "@auto-scripts",
        "post-update-cmd": "@auto-scripts",
        "prod": "@composer install --no-ansi --prefer-dist --no-dev --no-interaction --no-plugins --no-progress --no-scripts --optimize-autoloader",
        "staging": "@prod",
        "dev": "@composer install --no-ansi --no-interaction --no-progress --optimize-autoloader",
        "initial": [
            "@dev",
            "@db",
            "@fixtures",
            "@jwt"
        ],
        "db": [
            "php bin/console doctrine:database:create  --if-not-exists",
            "@migrate"
        ],
        "fixtures": "bin/console hautelook:fixtures:load --no-interaction",
        "migrate": "bin/console doctrine:migrations:migrate --allow-no-migration --no-interaction",
        "test": "vendor/bin/pest --parallel --coverage --min=11",
        "assets": "bin/console assets:install",
        "lint:phpcs": "vendor/bin/phpcs",
        "lint:container": "php bin/console lint:container",
        "lint:rector": "vendor/bin/rector process --dry-run --no-progress-bar --ansi",
        "lint:db:validate": "php bin/console doctrine:schema:validate",
        "lint:phpcs:fix": "vendor/bin/phpcbf",
        "lint:rector:fix": "vendor/bin/rector process --no-progress-bar --ansi",
        "lint": [
            "@lint:phpcs",
            "@lint:container",
            "@lint:rector",
            "@lint:db:validate"
        ],
        "jwt": "./.docker/jwt.sh"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
