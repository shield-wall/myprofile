{
    "name": "erison/myprofile",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.11",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "presta/sitemap-bundle": "^3.0",
        "sensio/framework-extra-bundle": "^6.0",
        "sentry/sentry-symfony": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/console": "^v5.2",
        "symfony/dotenv": "^v5.2",
        "symfony/expression-language": "^v5.2",
        "symfony/flex": "^1.3",
        "symfony/form": "^v5.2",
        "symfony/framework-bundle": "^v5.2",
        "symfony/http-client": "^v5.2",
        "symfony/mailgun-mailer": "^v5.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/proxy-manager-bridge": "^v5.2",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "^v5.2",
        "symfony/translation": "^v5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "^v5.2",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^v5.2",
        "symfonycasts/reset-password-bundle": "^1.2",
        "symfonycasts/verify-email-bundle": "^1.2",
        "transloadit/php-sdk": "^2.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.9",
        "rector/rector": "^0.12.18",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^5.2",
        "symplify/easy-coding-standard": "^10.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": "@auto-scripts",
        "post-update-cmd": "@auto-scripts",

        "initial": [
            "@dev",
            "@db",
            "@fixtures"
        ],
        "db": [
            "php bin/console doctrine:database:create  --if-not-exists",
            "@db:migration:apply"
        ],
        "lint": [
            "@lint:ecs",
            "@lint:rector",
            "@lint:twig",
            "@lint:container"
        ],
        "db:fixtures": "bin/console hautelook:fixtures:load --no-interaction",
        "db:migration:diff": "bin/console make:migration",
        "db:migration:apply": "bin/console doctrine:migrations:migrate --allow-no-migration --no-interaction",
        "db:validate": "php bin/console doctrine:schema:validate",
        "test":"php -d memory_limit=1024M bin/phpunit",
        "lint:ecs": "vendor/bin/ecs check src",
        "lint:ecs:fix": "vendor/bin/ecs check src --fix",
        "lint:rector": "vendor/bin/rector process --dry-run",
        "lint:rector:fix": "vendor/bin/rector process",
        "lint:twig": "php bin/console lint:twig templates",
        "lint:container": "php bin/console lint:container",
        "prod": [
            "@composer install --no-ansi --prefer-dist --no-dev --no-interaction --no-plugins --no-progress --no-scripts --optimize-autoloader",
            "@composer dump-autoload --optimize --no-dev"
        ],
        "staging": "@prod",
        "dev": "@composer install --no-ansi --no-interaction --no-progress --optimize-autoloader"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
